<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %} Welcome! {% endblock %}</title>
        {% block stylesheets %}
            <link rel="stylesheet" href="/pack/bootstrap.css">
            <script src="/pack/bootstrap.js"></script>
            <link rel="stylesheet" href="/pack/style.css">
        {% endblock %}
    </head>
    <body>
        <nav class="navbar navbar-expand-sm bg-dark navbar-dark">
            <ul class="navbar-nav ">
                <li class="nav-items">
                    <a class="nav-link" href=" {{ path('index') }} ">Home</a>
                </li>
                <li class="nav-items">
                    <a class="nav-link" href="{{ path('employee') }} ">Empoyees</a>
                </li>
                {# <li class="nav-items">
                    <a class="nav-link" href=" {{ path('service') }} ">Services</a>
                </li> #}
            </ul>
        </nav>
        
        <div class="container">
            {% block body %}{% endblock %}
        </div>

        {% block javascripts %}
            <script src="/pack/uncompressed_jquery.js"></script>
        {% endblock %}
    </body>
</html>